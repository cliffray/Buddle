<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/textView"
        android:layout_width="135dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="138dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="138dp"
        android:text="@string/text_tvText" />

    <EditText
        android:id="@+id/etProgramming"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-3dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="533dp"
        android:ems="10"
        android:hint="Programming"
        android:inputType="number"
        android:text="@string/etProgramming" />

    <EditText
        android:id="@+id/etDataStructure"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-3dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="486dp"
        android:ems="10"
        android:hint="Data Structure"
        android:inputType="number"
        android:text="@string/etDataStructure" />

    <EditText
        android:id="@+id/etAlgorithm"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-3dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="428dp"
        android:ems="10"
        android:hint="Algorithm"
        android:inputType="number"
        android:text="@string/etAlgorithm" />

    <Button
        android:id="@+id/btSubmit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-4dp"
        android:layout_marginTop="188dp"
        android:onClick="onSubmitClick"
        android:text="@string/btSubmit" />

</RelativeLayout>